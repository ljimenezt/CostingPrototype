<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
	version="2.2">

	<lifecycle>
		<phase-listener>co.informatix.erp.utils.SetFocusListener</phase-listener>
	</lifecycle>

	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>urlHome</from-outcome>
			<to-view-id>/home.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>gesLogin</from-outcome>
			<to-view-id>/login.xhtm</to-view-id>
		</navigation-case>
	</navigation-rule>
	<application>
		<locale-config>
			<default-locale>es</default-locale>
			<supported-locale>es</supported-locale>
			<supported-locale>en</supported-locale>
		</locale-config>
		<resource-bundle>
			<base-name>erp</base-name>
			<var>propErp</var>
		</resource-bundle>
		<resource-bundle>
			<base-name>PrototipoCostos</base-name>
			<var>propPrototipoCostos</var>
		</resource-bundle>
		<resource-bundle>
			<base-name>idioma</base-name>
			<var>mensaje</var>
		</resource-bundle>
		<message-bundle>idioma</message-bundle>
		<resource-bundle>
			<base-name>idiomaInformacionBase/idioma</base-name>
			<var>mensajeInformacionBase</var>
		</resource-bundle>
		<resource-bundle>
			<base-name>idiomaInventario/idioma</base-name>
			<var>mensajeInventario</var>
		</resource-bundle>
		<resource-bundle>
			<base-name>idiomaMenu/idioma</base-name>
			<var>mensajeMenu</var>
		</resource-bundle>
		<resource-bundle>
			<base-name>idiomaOrganizaciones/idioma</base-name>
			<var>mensajeOrganizaciones</var>
		</resource-bundle>
		<resource-bundle>
			<base-name>idiomaProcesos/idioma</base-name>
